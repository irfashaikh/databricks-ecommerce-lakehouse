{
  "name": "Bronze_Ingestion",
  "email_notifications": {
    "no_alert_for_skipped_runs": false
  },
  "webhook_notifications": {},
  "timeout_seconds": 0,
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "parameter",
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Workspace/Ecommerce_ETL/Bronze Layer/Autoloader_para_List",
        "source": "WORKSPACE"
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "Data_Ingestion",
      "depends_on": [
        {
          "task_key": "parameter"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "for_each_task": {
        "inputs": "{{tasks.parameter.values.Output_dataset}}",
        "task": {
          "task_key": "Data_Ingestion_iteration",
          "run_if": "ALL_SUCCESS",
          "notebook_task": {
            "notebook_path": "/Workspace/Ecommerce_ETL/Bronze Layer/Job_data_ingestion",
            "base_parameters": {
              "filename": "{{input.filename}}"
            },
            "source": "WORKSPACE"
          },
          "timeout_seconds": 0,
          "email_notifications": {},
          "webhook_notifications": {}
        }
      },
      "timeout_seconds": 0,
      "email_notifications": {}
    }
  ],
  "queue": {
    "enabled": true
  },
  "performance_target": "PERFORMANCE_OPTIMIZED"
}